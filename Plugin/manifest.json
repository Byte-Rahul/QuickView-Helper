{
    "manifest_version": 3,
    "name": "QuickView Helper",
    "description": "Instant Web Insights",
    "version": "1.0",
    "permissions": ["scripting", "tabs", "storage"],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": {
            "16": "images/logo.png"
        }
    },
    "icons":{
        "128": "images/logo.png"
    },
    "background": {
        "service_worker": "background/background.js",
        "type": "module"
    }
}